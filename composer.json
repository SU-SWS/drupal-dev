{
  "name": "su-sws/drupal-dev",
  "description": "Drupal dev dependencies manager",
  "type": "metapackage",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Mike Decker",
      "email": "pookmish@stanford.edu"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "codeception/codeception": "^4.1",
    "codeception/module-asserts": "^1.1",
    "codeception/module-phpbrowser": "^1.0",
    "codeception/module-webdriver": "^1.0",
    "drupal/config_inspector": ">=1.0",
    "drupal/core": ">=9.0",
    "drupal/core-dev": ">=9.0",
    "drupal/devel": ">=4.0",
    "drupal/devel_php": ">=1.1",
    "drupal/masquerade": ">=2.0",
    "league/oauth2-client": ">=2.4",
    "su-sws/stanford-caravan": "dev-8.x-3.x"
  },
  "extra": {
    "patches": {
      "drupal/simplesamlphp_auth": {
        "https://www.drupal.org/project/simplesamlphp_auth/issues/2124117": "https://www.drupal.org/files/issues/2020-02-01/2124117-6-reroll-14.patch"
      }
    }
  }
}
